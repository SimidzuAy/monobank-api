{
  "compilerOptions": {
    "target": "ES2019",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "lib": ["ES2019", "ESNext"],
    "esModuleInterop": true,
    "declaration": true,
    "outDir": "./build",
    "removeComments": false,
    "importHelpers": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "baseUrl": "src"
  }
}
